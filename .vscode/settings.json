{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".flyway-version": true,
        ".sqlfluff": true,
        ".python-version": false,
        ".pre-commit-config.yaml": true,
